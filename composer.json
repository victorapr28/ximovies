{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "ext-json": "*",
        "awobaz/compoships": "^1.1",
        "doctrine/dbal": "2.5.*",
        "doctrine/inflector": "1.1.0",
        "elasticsearch/elasticsearch": "5.x-dev",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "hashids/hashids": "2.0.4",
        "intervention/image": "^2.4",
        "jenssegers/agent": "^2.6",
        "torann/geoip": "^1.0",
        "laravel/framework": "5.8.*",
        "laravel/scout": "3.0.x-dev",
        "laravel/socialite": "3.0.x-dev",
        "laravel/tinker": "~1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/flysystem-rackspace": "^1.0",
        "league/omnipay": "^3.0",
        "mhetreramesh/flysystem-backblaze": "1.1.4",
        "omnipay/paypal": "^3.0",
        "omnipay/stripe": "^3.0",
        "pda/pheanstalk": "^3.1",
        "php-http/guzzle6-adapter": "1.1.1",
        "predis/predis": "^1.1",
        "sentry/sentry-laravel": "^1.1",
        "spatie/laravel-analytics": "1.4.1",
        "symfony/css-selector": "3.4.*",
        "symfony/dom-crawler": "3.4.19",
        "symfony/event-dispatcher": "^4.3",
        "symfony/translation": "^4.4",
        "teamtnt/laravel-scout-tntsearch-driver": "^2.0"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "itsgoingd/clockwork": "^4.0",
        "leafo/scssphp": "^0.7.7",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Common\\": "common/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
