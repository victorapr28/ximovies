{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "awobaz/compoships": "^1.1",
        "doctrine/dbal": "^2.10",
        "doctrine/inflector": "^2.0",
        "elasticsearch/elasticsearch": "5.x-dev",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "^6.3",
        "hashids/hashids": "2.0.4",
        "intervention/image": "^2.4",
        "jenssegers/agent": "^2.6",
        "torann/geoip": "^1.0",
        "laravel/framework": "^7.0",
        "laravel/scout": "^8.0",
        "laravel/socialite": "^4.4",
        "laravel/tinker": "^2.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/flysystem-rackspace": "^1.0",
        "league/omnipay": "^3.0",
        "mhetreramesh/flysystem-backblaze": "1.1.4",
        "omnipay/paypal": "^3.0",
        "omnipay/stripe": "^3.0",
        "pda/pheanstalk": "^3.1",
        "php-http/guzzle6-adapter": "1.1.1",
        "predis/predis": "^1.1",
        "sentry/sentry-laravel": "^1.1",
        "symfony/css-selector": "3.4.*",
        "symfony/dom-crawler": "3.4.19",
        "symfony/event-dispatcher": "^5.1",
        "symfony/translation": "^5.1",
        "teamtnt/laravel-scout-tntsearch-driver": "^8.3"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9",
        "itsgoingd/clockwork": "^4.0",
        "leafo/scssphp": "^0.7.7",
        "mockery/mockery": "^1.3",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Common\\": "common/"
        },
        "classmap": [
            "database"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}